{
  "name": "steam-manifest-manager",
  "version": "1.0.0",
  "description": "Steam Manifest Manager with database and web interface",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "start": "node server/index.js",
    "client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "db:migrate": "node server/db/migrate.js",
    "db:fix-length": "node server/db/fix-file-content-length.js"
  },
  "keywords": [
    "steam",
    "manifest",
    "manager"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "daisyui": "^4.6.1",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "vite": "^7.1.12"
  }
}
